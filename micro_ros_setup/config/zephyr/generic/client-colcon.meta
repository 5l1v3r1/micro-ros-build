{
    "names": {
        "rcl": {
            "cmake-args": [
                "-DBUILD_TESTING=OFF"
            ]
        }, 
        "rcutils": {
            "cmake-args": [
                "-DENABLE_TESTING=OFF",
                "-DRCUTILS_NO_FILESYSTEM=ON",
                "-DRCUTILS_NO_THREAD_SUPPORT=ON",
                "-DRCUTILS_AVOID_DYNAMIC_ALLOCATION=ON"
            ]
        },
        "microxrcedds_client": {
            "cmake-args": [
                "-DUCLIENT_ISOLATED_INSTALL=OFF",
                "-DUCLIENT_PIC=OFF",
                "-DUCLIENT_SUPERBUILD=OFF",
                "-DEXTERNAL_TRANSPORT_HEADER=/workspaces/uros_discovery/firmware/st-b-l475e-iot01a_extensions/microros/zephyr_usb_serial_transport.h",
                "-DEXTERNAL_TRANSPORT_SRC=/workspaces/uros_discovery/firmware/st-b-l475e-iot01a_extensions/microros/zephyr_usb_serial_transport.c"
            ]
        },
        "rmw_microxrcedds": {
            "cmake-args": [
                "-DRMW_UXRCE_MAX_NODES=1",
                "-DRMW_UXRCE_MAX_PUBLISHERS=1",
                "-DRMW_UXRCE_MAX_SUBSCRIPTIONS=1",
                "-DRMW_UXRCE_MAX_SERVICES=1",
                "-DRMW_UXRCE_MAX_CLIENTS=1",
                "-DRMW_UXRCE_MAX_HISTORY=4",
            ]
        }
    }
}
